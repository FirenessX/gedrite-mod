{
	"schemaVersion": 1,
	"id": "gedrite",
	"version": "${version}",
	"name": "Gedrite",
	"description": "Adds Gedrite in your world. Gedrite is a liquid and useful metal, you can even eat it! See more information in \"Gedrite Mod\" page in Curseforge/Modrinth",
	"authors": [
		"Fireness"
	],
	"contact": {
		"homepage": "",
		"sources": "https://github.com/FirenessX/gedrite-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/gedrite/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.gedrite.Gedrite"
		],
		"client": [
			"com.gedrite.GedriteClient"
		],
		"fabric-datagen": [
			"com.gedrite.GedriteDataGenerator"
		]
	},
	"mixins": [
		"gedrite.mixins.json",
		{
			"config": "gedrite.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}